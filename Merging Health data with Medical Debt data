#Merging Health data with Medical Debt data

install.packages("readr")
install.packages("readxl")
install.packages("dplyr")

library(readr)
library(readxl)
library(dplyr)

places_df <- read_csv("PLACES.csv")
medical_debt_df <- read_excel("medical_debt.xlsx")

#check initial columns for merger
head(places_df)
head(medical_debt_df)

#rename health data to match for merger
places_df <- places_df %>%
  rename(County_FIPS = CountyFIPS)

names(medical_debt_df) <- gsub(" ", "_", names(medical_debt_df))
view(medical_debt_df)

head(medical_debt_df$County_FIPS)
head(places_df$CountyFIPS)

#Filtering for needed counties

"County_FIPS" %in% names(places_df)
"County_FIPS" %in% names(medical_debt_df)

